# .env
# Конфигурация базы данных
DB_HOST=localhost
DB_PORT=5432
DB_RUSSIA=russia
DB_BELARUS=belarus
DB_KAZAKHSTAN=kazakhstan
DB_ADMIN_USER=admin
DB_ADMIN_PASSWORD=admin_password

# Конфигурация веб-панели
FLASK_SECRET_KEY=your_secret_key_here
